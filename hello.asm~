section .data
    msg db "Hello, World!", 10, 0    ; 10 = newline ('\n'), 0 = null terminator

section .text
    global main
    extern printf

main:
    sub rsp, 40          ; align stack to 16 bytes for Windows x64 calling convention
    mov rdi, msg         ; rdi = pointer to string (1st argument)
    xor rax, rax         ; rax must be 0 for variadic functions like printf
    call printf
    add rsp, 40          ; restore stack
    ret

